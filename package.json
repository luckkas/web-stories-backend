{
  "name": "web-stories-backend",
  "version": "1.0.0",
  "description": "A backend responsible to manager web stories.",
  "main": "main.js",
  "scripts": {
    "lint:fix": "eslint src --ext .ts --fix",
    "dev": "npm run lint && npm run format && tsnd  --ignore-watch node_modules -r tsconfig-paths/register --respawn src/main.ts",
    "start": "node dist/lib/main.js",
    "postbuild": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist/lib --copy-files --no-copy-ignored",
    "test": "npx jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write ."
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luckkas/web-stories-backend.git"
  },
  "keywords": [
    "domain-driven-desing",
    "clean-code"
  ],
  "author": "Lucas Melo <luckasmelo.dev@gmai.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luckkas/web-stories-backend/issues"
  },
  "homepage": "https://github.com/luckkas/web-stories-backend#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@swc/core": "^1.4.5",
    "@swc/jest": "^0.2.36",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.6",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.11.25",
    "@types/node-cache": "^4.2.5",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.3.*"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.19.1",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1"
  }
}
